{
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" }
  ],
  "functions": {
    "api/claude.js": {
      "maxDuration": 90
    }
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        { "key": "x-vercel-body-size-limit", "value": "10mb" }
      ]
    }
  ]
}
